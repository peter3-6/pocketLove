#charm-value {
   width: 446rpx;
   height: 88rpx;
   overflow: hidden;
   margin: 0 auto;
   border-top: 2rpx solid #d0cbd2;
   border-bottom: 2rpx solid #d0cbd2;
   margin-top: 6rpx;
}
#charm-value view {
   width: 49%;
   height: 100%;
   line-height: 88rpx;
   font-size: 126%;
   font-weight: bold;
   text-align: center;
   display: inline-block;
}
.charm {
   color: #6e3181;
}
.nickName, .charm-value {
   color: #ffb403;
}
.icon.w {
   width: 56%;
   height: 44rpx;
   margin: 16rpx auto;
}
.w {
   width: 100%;
   height: 100%;
   display: flex;
}
.icon image {
   width: 46rpx;
   height: 46rpx;
   display: block;
}
.icon text {
   width: 94%;
   height: 46rpx;
   line-height: 46rpx;
   font-size: 32rpx;
   font-family: monospace;
   display: block;
   overflow: hidden;
   white-space: nowrap;
   text-overflow: ellipsis;
   padding-left: 10rpx;
}
